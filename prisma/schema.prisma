// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
 url      = env("DATABASE_URL")
}

model Reservation {
  id          Int      @id @default(autoincrement())
  powerboat   String
  firstName   String
  lastName    String
  phoneNumber String
  annotation  String
  startDate   String
  endDate     String

  @@map("Reservation") 
}

model Request {
  id          Int      @id @default(autoincrement())
  powerboat   String
  firstName   String
  lastName    String
  phoneNumber String
  annotation  String
  startDate   String
  endDate     String

  @@map("Request")    
}

model Content {
  id         Int    @id @default(autoincrement())
  tagName    String @unique
  tagContent String

   @@map("Content")    
}